material {
    name : VideoMaterial,
    shadingModel : unlit,
    parameters : [
        {
            type : samplerExternal,
            name : videoTexture
        }
    ],
    requires : [ uv0 ]
}

fragment {
    void material(inout MaterialInputs material) {
        prepareMaterial(material);
        material.baseColor = texture(materialParams_videoTexture, getUV0());
    }
}
